<?xml version="1.0" encoding="utf-8"?>
<com.aram.android.flow.ReturnCoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_content"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:background="@android:color/background_light"
    tools:context="com.aram.android.flow.MainActivity">

    <com.aram.android.flow.VerticalViewPager
        android:id="@+id/container"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</com.aram.android.flow.ReturnCoordinatorLayout>
